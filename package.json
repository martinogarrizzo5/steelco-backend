{
  "name": "steelco-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server.ts",
    "buildServer": "npx tsc",
    "client": "cd client && npm run dev",
    "buildClient": "cd client && npm run build && rm -d -r ../build/client && mv -f ./dist/ ../build/client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "build": "npm run buildServer && npm run buildClient",
    "start": "node ./build/server.js",
    "install-dev": "npm install --dev-dependencies && cd client && npm install --dev-dependencies",
    "show-db": "prisma studio"
  },
