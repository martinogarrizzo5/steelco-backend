    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.21",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
